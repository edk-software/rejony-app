{% raw %}
<script>
    var rpChartData = {
    labels: [{% endraw %}{{ labels | raw }} {% raw %}],
    datasets: [
{
    label: [{% endraw %}{{ labels | raw }} {% raw %}],
    backgroundColor: [{% endraw %}{{ colors | raw }} {% raw %}],
    strokeColor: "#00965a",
    pointColor: "#00a65a",
    pointStrokeColor: "#c1c7d1",
    pointHighlightFill: "#fff",
    pointHighlightStroke: "rgba(220,220,220,1)",
    data: [{% endraw %}{{ data | raw }} {% raw %}]
},
    ]
};

    var rpChartCanvas = $("#participants-on-route").get(0).getContext("2d");
    var rpChartOptions = {
    segmentShowStroke: true,
    animateRotate: true,
    //Boolean - whether to make the chart responsive
    responsive: true,
    maintainAspectRatio: true,
    datasetFill: false,
    legend: {
        position: 'right'
    }
};
    var rpChart = new Chart(rpChartCanvas, {
    type: 'doughnut',
    data: rpChartData,
    options: rpChartOptions
});
</script>
{% endraw %}
