<div class="col-lg-12">
	<div class="box box-purple">
		<div class="box-header with-border">
			<i class="fa fa-map"></i>
			<h3 class="box-title">{{ 'Recently changed routes' | trans([], 'edk') }}</h3>
		</div>
		<div class="box-body no-padding">
			<table class="table table-hover">
				<tbody>
					{% for rr in routes %}
					<tr>
						<td><a href="{{ spath(routeInfoPath, {'id': rr.id} ) }}">{{ rr.name }}</a></td>
						<td width="25%">{{ rr.areaName }}</td>
						<td width="25%">{{ rr.updatedAt }}</td>
					</tr>
					{% endfor %}
				</tbody>	
			</table>
		</div>
	</div>
</div>